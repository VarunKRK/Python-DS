    "def pyramid(n):\n",
    "\n",
    "    #Outer loop for getting rows\n",
    "    for i in range(0, n):\n",
    "\n",
    "        #Inner loop for getting coloumns\n",
    "        for j in range(0, i+1):\n",
    "\n",
    "            # Printing '#'\n",
    "            print('#', end = '')\n",
    "\n",
    "        # Moving to next line\n",
    "        print('\\r')\n",
    "\n",
    "pyramid(5)"
   ]
  }
